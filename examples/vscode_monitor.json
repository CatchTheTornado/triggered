{
  "trigger": {
    "type": "ai",
    "config": {
      "name": "vscode-monitor",
      "prompt": "Check if VS Code is running",
      "tools": ["process_checker"]
    }
  },
  "action": {
    "type": "shell",
    "config": {
      "command": "echo 'VS Code is running!'"
    }
  },
  "params": {
    "process_name": "Code"
  }
} 